{
  "name": "preact-starter",
  "description": "Starter application with Preact, Atom, LESS modules and Babel.",
  "private": true,
  "author": "Adrian Unger",
  "version": "0.2.0",
  "repository": "",
  "scripts": {
    "start": "budo app/index.js --dir=public --live --pushstate --port 8080",
    "test": "npm run std",
    "std": "standard --verbose | snazzy"
  },
  "dependencies": {
    "atom-lens-reducer": "0.2.0",
    "check-arg-types": "1.1.0",
    "classless-component": "0.1.0",
    "preact": "8.2.6",
    "preact-portal": "1.1.2",
    "preact-router": "2.6.0",
    "query-string": "5.0.1",
    "redux": "3.7.2",
    "throttle-debounce": "1.0.1",
    "wasmuth": "1.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-jsx-import": "staydecent/babel-plugin-jsx-import",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24",
    "babelify": "^8.0.0",
    "browserify": "^14.5.0",
    "budo": "^10.0.4",
    "bundle-collapser": "^1.3.0",
    "clean-css": "^4.1.9",
    "common-shakeify": "^0.4.5",
    "purify-css": "^1.2.5",
    "redux-devtools-extension": "2.13.2",
    "snazzy": "^7.0.0",
    "split-require": "^2.0.3",
    "standard": "^10.0.3",
    "uglifyify": "^4.0.5"
  },
  "engines": {
    "npm": ">=3.0.0",
    "node": ">=6.0.0"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "standard": {
    "globals": [
      "Preact"
    ],
    "ignore": [
      "build"
    ]
  }
}
